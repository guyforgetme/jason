{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",	
	"landingPage": "/wp-admin/admin.php?page=twentig&path=website-templates",
	"features": {
		"networking": true
	},
	"steps": [
		{
			"step": "login"
		},
		{
            "step": "setSiteOptions",
            "options": {
                "blogname": "Demo"
            }
        },
		{
			"step": "installPlugin",
			"pluginZipFile": {
				"resource": "wordpress.org/plugins",
				"slug": "twentig"
			},
			"options": {
				"activate": true
			}
		}
	]
}
